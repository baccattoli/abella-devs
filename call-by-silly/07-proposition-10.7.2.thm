Theorem str_red_ans_strong_preponement_m : forall T T' U', 
   str_red_ans T' T -> ctx_cl_tm red_root_m T' U' 
   -> exists U, ctx_cl_tm red_root_m T U /\ str_red_ans U' U.
induction on 1. intros. case H1 (keep).
    case H3. case H2.
        case H5.
        apply is_answer_does_not_m_reduce to H4 H5.
        case H5.
            case H6.
            search.
            search.
    search.
    case H2.
        case H4.
        apply IH to H3 H4. search.
        search.